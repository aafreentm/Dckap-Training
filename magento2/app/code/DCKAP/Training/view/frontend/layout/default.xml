<?xml version="1.0"?>
	<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<body>
   <referenceContainer name="content">
    <block class="DCKAP\Training\Block\Banner" name="image-show" template="catalog/category/banner.phtml" />
    </referenceContainer>
    <referenceBlock name="footer_links">
         <block class="Magento\Framework\View\Element\Html\Link\Current" ifconfig="sectiondckap/groupdckap/fielddckap" name="mymodule-footer-link">
             <arguments>
                 <argument name="label" xsi:type="string">Create Customer</argument>
                 <argument name="path" xsi:type="string">mymodule/index/insert/</argument>
             </arguments>
         </block>
          <block class="Magento\Framework\View\Element\Html\Link\Current"  name="list-link">
             <arguments>
                 <argument name="label" xsi:type="string">Customers List</argument>
                 <argument name="path" xsi:type="string">mymodule/index/index/</argument>
             </arguments>
         </block>
     </referenceBlock>
         <!-- <move element="image-show" destination="page.top" after="breadcrumbs"/> -->
         <move element="image-show" destination="columns.top" after="page-title"/>

    
    
</body>
</page>